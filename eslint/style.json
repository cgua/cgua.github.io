{
  "extends": "./base.json",
  "rules": {
    "no-whitespace-before-property": 2,
    "no-trailing-spaces": 0,
    "comma-spacing": 0,
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2
  }
}