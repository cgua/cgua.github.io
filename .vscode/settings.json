{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "package-lock.json": false,
        "node_modules": false,
        "_pub": false,
        "coverage": false,
        ".gitginore": false
    },
    "eslint.enable": true,
    "json.schemas": [
        {
            "fileMatch": [
                "/eslint/*.json"
            ],
            "url": "/.vscode/eslintrc.schema.json"
        }
    ]
}